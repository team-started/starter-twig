{
    "name": "starterteam/starter-twig",
    "type": "typo3-cms-extension",
    "description": "TYPO3 data processing to handle twig templates",
    "license": "proprietary",
    "authors": [
        {
            "name": "Christian Wolfram",
            "email": "c.wolfram@chriwo.de"
        }
    ],
    "require": {
        "php": "^7.4",
        "pti/pti": "dev-feature/support_t3v11",
        "twig/twig": "^3",
        "typo3/cms-core": "^11.5",
        "starterteam/starter": "dev-main"
    },
	"suggest": {
		"starterteam/starter-twig-news": "4.x-dev"
	},
	"autoload": {
        "psr-4": {
            "StarterTeam\\StarterTwig\\": "Classes"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.x-dev"
        },
        "typo3/cms": {
            "extension-key": "starter_twig"
        }
    }
}
