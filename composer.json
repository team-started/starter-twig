{
    "name": "starterteam/starter-twig",
    "type": "typo3-cms-extension",
    "description": "TYPO3 data processing to handle twig templates",
    "license": "proprietary",
    "authors": [
        {
            "name": "Christian Wolfram",
            "email": "c.wolfram@chriwo.de"
        }
    ],
    "require": {
        "php": "^7.4 || ^8.1",
        "pti/pti": "dev-feature/support_t3v11 || ^3 || dev-master",
        "twig/twig": "^3",
        "typo3/cms-core": "^11.5",
        "starterteam/starter": "^4.5"
    },
    "suggest": {
        "starterteam/starter-twig-news": "Processing package for EXT:news to work with Twig templates"
    },
    "autoload": {
        "psr-4": {
            "StarterTeam\\StarterTwig\\": "Classes"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.5-dev"
        },
        "typo3/cms": {
            "extension-key": "starter_twig",
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}
