{
    "name": "starterteam/starter-twig",
    "type": "typo3-cms-extension",
    "description": "TYPO3 data processing to handle twig templates",
    "license": "proprietary",
    "authors": [
        {
            "name": "Christian Wolfram",
            "email": "c.wolfram@chriwo.de"
        }
    ],
    "require": {
		"php": "^8.3",
		"pti/pti": "^4.0 || dev-main",
		"twig/twig": "^3.14.2",
		"typo3/cms-core": "^12.4 || ^13.0",
		"starterteam/starter": "^4.5 || dev-main"
    },
    "suggest": {
        "starterteam/starter-twig-news": "Processing package for EXT:news to work with Twig templates"
    },
    "autoload": {
        "psr-4": {
            "StarterTeam\\StarterTwig\\": "Classes"
        }
    },
    "config": {
        "vendor-dir": ".Build/vendor",
        "bin-dir": ".Build/bin",
        "allow-plugins": {
            "typo3/class-alias-loader": true,
            "typo3/cms-composer-installers": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-main": "4.5-dev"
        },
        "typo3/cms": {
            "extension-key": "starter_twig",
            "app-dir": ".Build",
            "web-dir": ".Build/public"
        }
    }
}
